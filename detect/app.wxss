/**app.wxss**/

@import './dist/weui.wxss';
@import './dist/index.wxss';

.btn-area {
  margin-top: 25rpx;
  box-sizing: border-box;
  width: 100%;
  padding: 0 30rpx;
}

.container {
  background: #F9F9F9;
  overflow: hidden;
  min-height: 100vh;
  box-sizing: border-box;
  padding: 10rpx 0;
}
.container::before {
  position: fixed;
  top: 0;
  left: 0;
  content: ' ';
  width: 100%;
  height: 1rpx;
  background-color: #e2e2e2;
  z-index: 5;
}
.doc-title {
  position: relative;
  padding: 20rpx 0;
  margin: 15rpx 30rpx;
  line-height: 50rpx;
  font-size: 35rpx;
  color: #666;
}
.doc-description {
  margin: 14px 0;
  padding: 0 15px;
  font-size: 14px;
  line-height: 20px;
  color: #666;
}


